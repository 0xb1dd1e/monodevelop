<?xml version="1.0"?>
<Template	Originator="Michael Hutchinson"
			Created="2006/07/13"
			LastModified="2006/07/13">

	<!-- Template Header -->
	<TemplateConfiguration>
		<_Name>Web Service</_Name>
		<Category>ASP.NET</Category>
		<Icon>md-html-file-icon</Icon>
		<LanguageName>*</LanguageName>
		<_Description>Creates an ASP.NET web service.</_Description>
	</TemplateConfiguration>

	<!-- Template Content -->	
	<TemplateFiles>
		<AspNetFile name="${Name}.asmx" BuildAction="FileCopy">
			<FileText>
<![CDATA[<%@ WebService Language="C#" Class="${Namespace}.${Name}" %>
${CodeRegion:script1}
]]>
			</FileText>
			<CodeTranslationFile TagName="script1">
<![CDATA[using System;
using System.Web.Services;

namespace ${Namespace}
{
	class ${Name}
	{
	}
}]]>
			</CodeTranslationFile>
		</AspNetFile>
	</TemplateFiles>

	<FileOptions />

</Template>
