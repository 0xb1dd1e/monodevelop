<Addin id        = "VersionControl.Subversion"
       namespace = "MonoDevelop"
       name      = "Subversion support for the Version Control Addin"
       author    = "Joshua Tauberer"
       copyright = "GPL"
       url       = "http://taubz.for.net/code/diff"
       description = "Subversion support for the Version Control Addin"
       category    = "IDE extensions"
       version   = "0.13.0">
 
	<Runtime>
		<Import assembly="MonoDevelop.VersionControl.Subversion.dll"/>
	</Runtime>

	<Dependencies>
	    <Addin id="Core" version="0.13.0"/>
	    <Addin id="Core.Gui" version="0.13.0"/>
		<Addin id="Projects" version="0.13.0"/>
	    <Addin id="Ide" version="0.13.0"/>
	    <Addin id="VersionControl" version="0.13.0"/>
	</Dependencies>
	
	<Extension path = "/MonoDevelop/VersionControl/VersionControlSystems">
		<Class class = "MonoDevelop.VersionControl.Subversion.SubversionVersionControl" />
	</Extension>

</Addin>
	
