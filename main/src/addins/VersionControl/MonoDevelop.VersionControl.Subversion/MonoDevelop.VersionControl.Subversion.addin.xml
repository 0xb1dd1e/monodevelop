<Addin id        = "VersionControl.Subversion"
       namespace = "MonoDevelop"
       name      = "Subversion support for the Version Control Add-in"
       author    = "Joshua Tauberer"
       copyright = "GPL"
       url       = "http://taubz.for.net/code/diff"
       description = "Subversion support for the Version Control Add-in"
       category    = "Version Control"
       version   = "1.9.0">
 
	<Runtime>
		<Import assembly="MonoDevelop.VersionControl.Subversion.dll"/>
	</Runtime>

	<Dependencies>
	    <Addin id="Core" version="1.9.0"/>
	    <Addin id="Core.Gui" version="1.9.0"/>
		<Addin id="Projects" version="1.9.0"/>
	    <Addin id="Ide" version="1.9.0"/>
	    <Addin id="VersionControl" version="1.9.0"/>
	</Dependencies>
	
	<Extension path = "/MonoDevelop/VersionControl/VersionControlSystems">
		<Class class = "MonoDevelop.VersionControl.Subversion.SubversionVersionControl" />
	</Extension>

</Addin>
	
